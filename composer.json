{
    "name": "webignition/node-jslint-wrapper",
    "description": "Wrapper for the nodejs jslint wrapper. I heard you liked wrappers ...",
    "keywords": ["jslint", "wrapper"],
    "homepage": "https://github.com/webignition/node-jslint-wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "webignition\\Tests": "tests/",
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "webignition/web-resource-service":">=1.1,<2.0",
        "guzzlehttp/guzzle":">=5.2.0,<5.3",
        "webignition/node-jslint-output-parser":">=1.0.6,<2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "icanboogie/inflector":"1.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "~0.9",
        "php-mock/php-mock-mockery": "1.1.0"
    },
    "minimum-stability":"stable"
}